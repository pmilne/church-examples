import church.lang.operators.Streams;
import church.lang.operators.Relational;
import church.lang.operators.Arithmetic;

import church.math.Fraction;

main(String[]: args) = {
    r1 = fraction(1, 2);
    r2 = fraction(3, 4);
    r3 = -r1;
    r4 = r1 + r2 * r3 - r3;
    output << r4 << "\n";
    assert r4 == fraction(5, 8);
};
